{
    "builds": [
      {
        "src": "invoice_project/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "50mb", "runtime": "python3.9" }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "invoice_project/wsgi.py"
      }
    ]
  }
  